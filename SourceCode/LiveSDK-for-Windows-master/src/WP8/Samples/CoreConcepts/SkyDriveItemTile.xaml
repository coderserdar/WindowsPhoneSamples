<UserControl x:Class="CoreConcepts.SkyDriveItemTile"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="70" d:DesignWidth="460">
    
    <Grid x:Name="LayoutRoot" HorizontalAlignment="Left">
        <Button Name="ButtonItem" Height="70" HorizontalAlignment="Left"  Width="460" BorderBrush="Transparent">
            <TextBlock Text="Text" HorizontalAlignment="Left" />
        </Button>
    </Grid>
</UserControl>
